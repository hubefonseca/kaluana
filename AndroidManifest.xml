<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="thesis.mobilis" android:versionCode="1" android:versionName="1.0.0">
	<application android:icon="@drawable/icon" android:label="@string/app_name">
		<activity android:label="@string/app_name" android:name=".Application">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<service android:name=".examples.helloworld.services.AdapterService">
			<intent-filter>
				<action android:name="thesis.mobilis.services.iMobService"></action>
			</intent-filter>
		</service>
	<service android:name=".examples.helloworld.services.MobService"><intent-filter><action android:name="thesis.mobilis.examples.helloworld.services.iMobService"></action>
</intent-filter>
</service>

<service android:name=".impl.control.ComponentLoader"><intent-filter><action android:name="thesis.mobilis.api.control.IComponentLoader"></action>
</intent-filter>
</service>
<service android:name=".examples.pingpong.PingService"><intent-filter><action android:name="thesis.mobilis.examples.pingpong.IPingService"></action>
</intent-filter>
</service>
<service android:name=".examples.pingpong.PongService"><intent-filter><action android:name="thesis.mobilis.examples.pingpong.IPongService"></action>
</intent-filter>
</service>
</application>
</manifest> 